{
  "_version": "1.32.0",
  "sap.app": {
    "id": "com.atlascopco.pt.portal.myopenorders",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "/srv_api/order/",
        "type": "OData",
        "settings": {
          "annotations": ["annotation"],
          "odataVersion": "4.0",
          "localUri": "localService/metadata.xml"
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      }
    },
    "offline": false,
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "ui5template.fiorielements.v4.lrop",
      "version": "1.0.0"
    },
    "crossNavigation": {
      "inbounds": {
        "intent": {
          "signature": {
            "parameters": {
              "SoldTo": {
                "required": "false",
                "defaultValue": {
                  "value": "|organization|",
                  "format": "plain"
                }
              },
              "SoldToName": {
                "required": "false",
                "defaultValue": {
                  "value": "|organizationName|",
                  "format": "plain"
                }
              }
            },
            "additionalParameters": "allowed"
          },
          "semanticObject": "myopenorders",
          "action": "display",
          "title": "{{flpTitle}}",
          "subTitle": "{{flpSubtitle}}",
          "icon": "sap-icon://customer-order-entry"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "resources": {
      "js": [],
      "css": []
    },
    "dependencies": {
      "minUI5Version": "1.97.0",
      "libs": {
        "sap.ui.core": {},
        "sap.fe.templates": {},
        "sap.f": {}
      }
    },
    "models": {
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.suite.ui.generic.template.ListReport.view.ListReport": {
            "controllerName": "com.atlascopco.pt.portal.myopenorders.ext.controller.ListReportExt"
          }
        },
        "sap.ui.viewExtensions": {
          "sap.suite.ui.generic.template.ListReport.view.ListReport": {
            "SmartFilterBarControlConfigurationExtension|Order": {
              "className": "sap.ui.core.Fragment",
              "fragmentName": "com.atlascopco.pt.portal.myopenorders.ext.fragment.ExtensionFilter",
              "type": "XML"
            }
          }
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "OrderList",
          "target": ["OrderList"]
        },
        {
          "pattern": "Order({key}):?query:",
          "name": "OrderObjectPage",
          "target": ["OrderList", "OrderObjectPage"]
        },
        {
          "pattern": "Order({key})/Deliveries({key2}):?query:",
          "name": "DeliveriesObjectPage_Deliveries",
          "target": [
            "OrderList",
            "OrderObjectPage",
            "DeliveriesObjectPage_Deliveries"
          ]
        }
      ],
      "targets": {
        "OrderList": {
          "type": "Component",
          "id": "OrderList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Order",
              "variantManagement": "Page",
              "navigation": {
                "Order": {
                  "detail": {
                    "route": "OrderObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "enableExport": false
                  }
                }
              },
              "initialLoad": "Auto"
            }
          },
          "controlAggregation": "beginColumnPages",
          "contextPattern": ""
        },
        "OrderObjectPage": {
          "type": "Component",
          "id": "OrderObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "entitySet": "Order",
              "navigation": {
                "Deliveries": {
                  "detail": {
                    "route": "DeliveriesObjectPage_Deliveries"
                  }
                }
              },
              "controlConfiguration": {
                "LineItems/@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "enableExport": false
                  }
                },
                "Deliveries/@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "enableExport": false
                  }
                },
                "Invoices/@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "enableExport": false
                  }
                }
              }
            }
          },
          "controlAggregation": "midColumnPages",
          "contextPattern": "/Order({key})"
        },
        "DeliveriesObjectPage_Deliveries": {
          "type": "Component",
          "id": "DeliveriesObjectPage_Deliveries",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Deliveries",
              "navigation": {},
              "controlConfiguration": {
                "DeliveryItems/@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "enableExport": false
                  }
                }
              }
            }
          },
          "controlAggregation": "endColumnPages",
          "contextPattern": "/Order({key})/Deliveries({key2})"
        }
      },
      "config": {
        "flexibleColumnLayout": {
          "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
          "defaultThreeColumnLayoutType": "ThreeColumnsMidExpanded"
        },
        "routerClass": "sap.f.routing.Router"
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "rootView": {
      "viewName": "sap.fe.templates.RootContainer.view.Fcl",
      "type": "XML",
      "async": true,
      "id": "appRootView"
    }
  },
  "sap.platform.abap": {
    "_version": "1.1.0",
    "uri": ""
  },
  "sap.platform.hcp": {
    "_version": "1.1.0",
    "uri": ""
  },
  "sap.fiori": {
    "_version": "1.1.0",
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "service": "myorders.service"
  }
}
